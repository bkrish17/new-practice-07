//remove duplicate of the sorted array 

#include <iostream>
using namespace std;
int remdups(int arr[],int n)
{
    int res=1;
    for(int i=1;i<n;i++)
    {
        if(arr[i]!=arr[res-1])
        {
            arr[res]=arr[i];
            res++;
        }
    }
    return res;
}
int main()
{
    int arr[]={9,9,8,8,7,7,6,6,5,5,4,4};
    int n=sizeof(arr)/sizeof(arr[0]);
    int newsize=remdups(arr,n);
    cout<<"array after removing duplicates"<<endl;
    for(int i=0;i<newsize;i++)
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}

